{
  "plugins": [
    "postcss-flexbugs-fixes",
    "postcss-nesting",
    [
      "postcss-preset-env",
      {
        "autoprefixer": {
          "flexbox": "no-2009"
        },
        "stage": 2,
        "features": {
          "nesting-rules": { "preserve": false }
        }
      }
    ]
  ]
}
